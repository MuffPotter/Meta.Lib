<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <RootNamespace>Meta.Lib</RootNamespace>
    <AssemblyName>Meta.Lib</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/dzaliznyak/Meta.Lib</PackageProjectUrl>
    <PackageLicenseFile></PackageLicenseFile>
    <Copyright>Copyright © 2020 Denys Zaliznyak</Copyright>
    <Authors>Denys Zaliznyak</Authors>
    <RepositoryUrl>https://github.com/dzaliznyak/Meta.Lib.git</RepositoryUrl>
    <Description>A set of classes and modules for .Net development including MetaPubSub - Publish-Subscribe pattern implementation</Description>
    <PackageReleaseNotes>Publisher-Subscriber module is ready</PackageReleaseNotes>
    <PackageTags>publish-subscribe utility csharp event-driven event-hub publisher-subscriber library pubsub</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="System.Collections.Immutable" Version="1.7.0" />
  </ItemGroup>

</Project>
